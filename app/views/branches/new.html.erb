<h3><%= @branch.history[:title] if @branch %></h3>
<p><%= @branch.history[:content] if @branch %></p>

<%= form_for @new_branch, :url => create_branch_path(@tree, @branch) do |f| %>

  <%= f.label :content %><br />
  <%= f.text_area :content %><br />
  <%= f.submit "Create Branch"  %>

<% end %>





